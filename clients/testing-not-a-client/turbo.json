{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["$TURBO_EXTENDS$"],
  "tasks": {
    "build": {
      "env": ["$TURBO_EXTENDS$", "TINYBIRD_TOKEN", "POSTMARK_API_KEY"],
      "outputs": ["$TURBO_EXTENDS$", "public/static/**"]
    },
    "prebuild": {
      "outputs": ["generated/**"]
    }
  }
}
