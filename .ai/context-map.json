{
  "repository": "your-dedicated-marketer",
  "type": "marketing-website",
  "framework": "nextjs-app-router",
  "structure": {
    "app": "Next.js App Router pages and routes",
    "components": "React components - UI components in ui/ subfolder",
    "features": "Feature modules with api/, components/, lib/ subfolders",
    "lib": "Shared utilities, actions, and helpers"
  },
  "keyFiles": {
    "layout": "app/layout.tsx",
    "homepage": "app/page.tsx",
    "styles": "app/globals.css",
    "providers": "app/providers.tsx"
  },
  "commonPatterns": {
    "serverActions": "lib/actions/",
    "apiRoutes": "app/api/",
    "featureStructure": "features/[name]/{index.ts, api/, components/, lib/}"
  }
}
