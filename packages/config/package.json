{
  "name": "@repo/config",
  "version": "1.0.0",
  "private": true,
  "description": "Shared configuration packages for ESLint, TypeScript, Jest, etc.",
  "workspaces": [
    "eslint-config",
    "typescript-config",
    "jest-config"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  }
}
