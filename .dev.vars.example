# Local development environment variables for Cloudflare
# Copy this to .dev.vars and fill in your values
# .dev.vars is automatically loaded by wrangler and is in .gitignore

# Upstash Redis (for rate limiting)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_token_here

# Resend (for email)
RESEND_API_KEY=your_resend_api_key_here

# Sentry (for error tracking)
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ORG=your_sentry_org_here
SENTRY_PROJECT=your_sentry_project_here
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here

# Analytics (optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_MICROSOFT_CLARITY_ID=
NEXT_PUBLIC_HOTJAR_ID=
